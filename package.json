{
	"name": "wallet-you",
	"version": "1.0.0",
	"main": "node_modules/expo/AppEntry.js",
	"engines": {
		"node": ">=18.0.0"
	},
	"private": true,
	"scripts": {
		"start:dev": "NODE_ENV=development expo start",
		"start:prod": "NODE_ENV=production expo start",
		"android:dev": "NODE_ENV=development expo start --android",
		"android:prod": "NODE_ENV=production expo start --android",
		"ios:dev": "NODE_ENV=development expo start --ios",
		"ios:prod": "NODE_ENV=production expo start --ios",
		"build-android": "npx eas build --platform android",
		"submit-android": "eas submit --platform android",
		"test": "jest",
		"run-eslint": "eslint \"**/*.{ts,tsx}\"",
		"prettier": "npx prettier --write --ignore-unknown .",
		"disable-crlf": "git config core.autocrlf false",
		"remove-file-cache": "git rm --cached -r .",
		"git-reset": "git reset --hard"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "1.17.11",
		"@react-navigation/bottom-tabs": "^6.5.7",
		"@react-navigation/native": "^6.1.6",
		"@react-navigation/native-stack": "^6.9.12",
		"@react-navigation/stack": "^6.3.16",
		"@tatumcom/js": "^1.4.22",
		"axios": "^1.4.0",
		"currency.js": "^2.0.4",
		"expo": "~48.0.15",
		"expo-checkbox": "~2.3.1",
		"expo-clipboard": "~4.1.2",
		"expo-constants": "~14.2.1",
		"expo-font": "~11.1.1",
		"expo-linear-gradient": "~12.1.2",
		"expo-local-authentication": "~13.3.0",
		"expo-localization": "~14.1.1",
		"expo-network": "~5.2.1",
		"expo-secure-store": "~12.1.1",
		"expo-splash-screen": "~0.18.2",
		"expo-status-bar": "~1.4.4",
		"i18next": "^22.5.0",
		"jest-expo": "^48.0.2",
		"moment": "^2.29.4",
		"moti": "^0.25.3",
		"react": "18.2.0",
		"react-i18next": "^12.3.1",
		"react-native": "0.71.8",
		"react-native-dotenv": "^3.4.8",
		"react-native-gesture-handler": "~2.9.0",
		"react-native-localize": "^3.0.0",
		"react-native-reanimated": "~2.14.4",
		"react-native-responsive-screen": "^1.4.2",
		"react-native-safe-area-context": "4.5.0",
		"react-native-screens": "~3.20.0",
		"react-native-svg": "13.4.0",
		"react-native-svg-charts": "^5.4.0",
		"styled-components": "5.3.5",
		"zustand": "^4.3.8"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/preset-typescript": "^7.21.5",
		"@testing-library/jest-native": "^5.4.2",
		"@testing-library/react-native": "^12.1.2",
		"@types/d3-shape": "^3.1.1",
		"@types/jest": "^29.5.1",
		"@types/react": "~18.0.14",
		"@types/react-native-svg-charts": "^5.0.12",
		"@types/react-test-renderer": "^18.0.0",
		"@types/styled-components": "^5.1.26",
		"@types/styled-components-react-native": "5.2.1",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"babel-jest": "^29.5.0",
		"eslint": "^8.41.0",
		"eslint-plugin-react": "^7.32.2",
		"jest": "^29.2.1",
		"metro-react-native-babel-preset": "^0.76.4",
		"react-test-renderer": "^18.2.0",
		"typescript": "^4.9.4"
	},
	"jest": {
		"preset": "jest-expo",
		"moduleNameMapper": {
			"@src/(.*)": "<rootDir>/src/$1"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)|@react-native-async-storage/async-storage"
		],
		"setupFilesAfterEnv": [
			"@testing-library/jest-native/extend-expect",
			"<rootDir>/__test__/setup.ts"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
